{
    "name": "btovis",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "dependencies": {
        "@emotion/react": "^11.11.3",
        "@emotion/styled": "^11.11.0",
        "@fontsource/open-sans": "^5.0.24",
        "@mui/material": "^5.15.10",
        "@mui/x-date-pickers": "^6.19.4",
        "bootstrap": "^5.3.2",
        "dayjs": "^1.11.10",
        "plotly.js": "^2.29.1",
        "react": "^18.2.0",
        "react-bootstrap": "^2.10.1",
        "react-bootstrap-icons": "^1.11.3",
        "react-dom": "^18.2.0",
        "react-floating-action-button": "^1.0.5",
        "react-plotly.js": "^2.6.0",
        "react-resizable": "^3.0.5",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.4.2",
        "@testing-library/react": "^14.2.1",
        "@types/react": "^18.2.43",
        "@types/react-dom": "^18.2.17",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.14.0",
        "@vitejs/plugin-react": "^4.2.1",
        "@vitest/coverage-v8": "^1.3.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-standard-with-typescript": "^43.0.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-n": "^16.6.2",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "husky": "^8.0.0",
        "jsdom": "^24.0.0",
        "license-checker-rseidelsohn": "^4.3.0",
        "lint": "^1.1.2",
        "lint-staged": "^15.2.2",
        "prettier": "^3.2.5",
        "typescript": "^5.3.3",
        "vite": "^5.0.8",
        "vitest": "^1.2.2"
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build --emptyOutDir",
        "prepare": "husky install",
        "lint": "eslint --fix \"devfiles/src/**/*.{js,jsx,ts,tsx}\"",
        "format": "prettier --write --ignore-unknown devfiles/src/**/*.{json,js,ts,jsx,tsx,html,css}",
        "preview": "vite preview",
        "test": "vitest",
        "coverage": "vitest run --coverage --coverage.reportOnFailure --coverage.reportsDirectory ../coverage --coverage.reporter text --coverage.reporter html --coverage.reporter clover --coverage.reporter json --coverage.reporter json-summary",
        "check-staged": "npm run build && npm run format && npm run lint",
        "license": "license-checker-rseidelsohn --production --json --out devfiles/src/assets/license.json && node scripts/license.js devfiles/src/assets/license.json devfiles/src/assets/license.txt"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --quiet --fix"
        ],
        "*.{json,js,ts,jsx,tsx,html,css}": [
            "prettier --write --ignore-unknown"
        ],
        "!*": [
            "npm run build"
        ]
    }
}
